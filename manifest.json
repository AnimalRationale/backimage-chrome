{
  "name": "Background Image Chnager",
  "version": "0.1",
  "description": "Chrome Extension for changing background images.",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Background Changer"
  },
  "permissions": [
    "activeTab"
  ],
  "manifest_version": 1
}